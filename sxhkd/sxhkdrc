# pkill -USR1 -x sxhkd
# launching applications
super + o; w
	firefox

super + o; d
	discord

super + o; m
	spotify

super + o; e
	code

super + o; r
	termite -e ranger

# media control
super + m; {space, Right, l, Left, h}
  playerctl -p spotify {play-pause, next, next, previous, previous}

# dunst

super + alt + space
	dunstctl close

super + alt + shift + space
	dunstctl close-all

super + ctrl + h
	dunstctl history-pop
